{
  "version": 2,
  "builds": [
    {
      "src": "src/main.tsx",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist",
        "base": "/",
        "routes": [
          {
            "src": "/.*",
            "dest": "/index.html"
          }
        ]
      }
    }
  ],
  "routes": [{ "src": "/favicon.ico", "dest": "/favicon.ico" }]
}
